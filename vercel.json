{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/static-build" },
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/users", "dest": "server.js" },
    { "src": "/api/login", "dest": "server.js" },
    { "src": "/(.*)", "dest": "app.js" }
  ]
}